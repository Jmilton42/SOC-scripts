<decoder name="snoopy">
        <prematch>uid:\d:*\ssid:\d*\stty:\.*\scwd:\.*\sfilename:\.*</prematch>
</decoder>

<decoder name="snoopy_child">
        <parent>snoopy</parent>
        <regex type="PCRE2">tty:(\S+)</regex>
        <order>tty</order>
</decoder>

<decoder name="snoopy_child">
        <parent>snoopy</parent>
        <regex type="PCRE2">uid:(\S+)</regex>
        <order>uid</order>
</decoder>

<decoder name="snoopy_child">
        <parent>snoopy</parent>
        <regex type="PCRE2">filename:(\S+)\]</regex>
        <order>filename</order>
</decoder>

<decoder name="snoopy_child">
        <parent>snoopy</parent>
        <regex type="PCRE2">cwd:(\S+)</regex>
        <order>cwd</order>
</decoder>

<decoder name="snoopy_child">
        <parent>snoopy</parent>
        <regex type="PCRE2">\]:\s(.*)$</regex>
        <order>cmd</order>
</decoder>

<decoder name="redshell">
        <prematch>^REDTRAP</prematch>
</decoder>

<decoder name="redshell_child">
        <parent>redshell</parent>
        <regex>^REDTRAP\s(\S*)\s</regex>
        <order>day</order>
</decoder>

<decoder name="redshell_child">
        <parent>redshell</parent>
        <regex>(\d\d:\d\d:\d\d\s\SM)</regex>
        <order>time</order>
</decoder>

<decoder name="redshell_child">
        <parent>redshell</parent>
        <regex>--\s(\.+)</regex>
        <order>cmd</order>
</decoder>